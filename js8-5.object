<!DOCTYPE html>
<html lang="ja">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>8-5</title>
  </head>
  <body>
    <script>
      function Akabane(no, name) {
        this.no = no;
        this.name = name;
        // this.dep = dep;
        this.aisatu = function () {
          document.write(
            "初めまして、" + this.no + "番の" + this.name + "です" + "<br>"
          );
        };
      }
      let seito01 = new Akabane("10", "上村");
      seito01.aisatu();
    </script>
  </body>
