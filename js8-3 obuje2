      let fruit = {
        apple: "りんご",
        orange: "みかん",
        banana: "バナナ",
        peach: "もも",
      };
      for (let key in fruit) {
        document.write(key + " : ");
        document.write(fruit[key] + "<br>");
      }
    </script>
